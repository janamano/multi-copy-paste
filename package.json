{
	"name": "multi-copy-paste",
	"displayName": "Multi Copy Paste",
	"description": "Copy multiple things at once",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.36.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "extension.copy",
				"title": "Copy"
			},
			{
				"command": "extension.paste",
				"title": "Paste"
			}
		],
		"keybindings":[
			{
				"command": "extension.copy",
				"key": "ctrl+c",
				"mac": "cmd+c",
				"when": "textInputFocus"
			},
			{
				"command": "extension.paste",
				"key": "ctrl+v",
				"mac": "cmd+v",
				"when": "textInputFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.36.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.0.2"
	}
}
